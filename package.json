    "svg-inline-loader": "^0.8.0",
    "typescript": "^5.6.3",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.4.0",
    "vite-plugin-env-compatible": "^2.0.1",
    "vite-plugin-html": "3.2.2",
    "vite-plugin-svgo": "^1.6.2"
